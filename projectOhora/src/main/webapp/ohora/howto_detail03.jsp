			 // go_top 버튼 스크립트
			$(document).ready(function() {
			    // go_top 버튼 클릭 시 상단으로 부드럽게 이동
			    $('.go_top').on('click', function() {
			        $('html, body').animate({ scrollTop: 0 }, 400);
			        return false;
			    });
			    
			    // 스크롤 이벤트에 따라 go_top 버튼 표시
			    $(window).scroll(function() {
			        if ($(this).scrollTop() > 200) {
			            $('#floating').fadeIn();
			        } else {
			            $('#floating').fadeOut();
			        }
			    });
			});
			</script>
